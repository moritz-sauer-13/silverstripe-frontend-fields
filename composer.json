{
    "name": "moritz-sauer-13/silverstripe-frontend-fields",
    "description": "Upload Image / File from Silverstripe frontend.",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/hudhaifas/silverstripe-frontend-fields",
    "keywords": ["silverstripe", "frontend", "field", "image", "file", "upload"],
    "license": "MIT",
    "authors": [
        {
            "name": "Hudhaifa Shatnawi",
            "email": "me@hudhaifa.com"
        }
    ],
    "support": {
        "issues": "https://github.com/hudhaifas/silverstripe-frontend-fields/issues"
    },
    "require": {
        "silverstripe/framework": "^6",
        "silverstripe/cms": "^6",
        "silverstripe/vendor-plugin": "^3"
    },
    "require-dev": {
      "rector/rector": "^2.2",
      "phpstan/phpstan": "^2.1",
      "wernerkrauss/silverstripe-rector": "^1.0"
    },
    "extra": {
        "installer-name": "frontend-fields",
        "expose": [
            "res/css",
            "res/images",
            "res/js"
        ]
    },
    "autoload": {
        "psr-4": {
            "HudhaifaS\\Forms\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
